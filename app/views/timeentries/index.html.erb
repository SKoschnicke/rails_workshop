<p id="notice"><%= notice %></p>

<h1>Listing Timeentries</h1>

<table>
  <thead>
    <tr>
      <th>Duration</th>
      <th>Date</th>
      <th>User</th>
      <th>Client</th>
      <th>Comment</th>
      <th>Tags</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timeentries.each do |timeentry| %>
      <tr>
        <td><%= timeentry.duration %></td>
        <td><%= timeentry.date %></td>
        <td><%= timeentry.user.name %></td>
        <td><%= timeentry.contract.try :client%></td>
        <td><%= timeentry.comment %></td>
        <td><%= timeentry.tags_string %></td>
        <td><%= link_to 'Show', timeentry %></td>
        <td><%= link_to 'Edit', edit_timeentry_path(timeentry) %></td>
        <td><%= link_to 'Destroy', timeentry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timeentry', new_timeentry_path %>
